// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.vscode/": true,
        "**/.project": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules/": true,
        "**/typings/": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    }
}